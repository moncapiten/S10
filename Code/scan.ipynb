    "import tdwf\n",
    "ad2 = tdwf.AD2()\n",
    "ad2.vdd = 3.3\n",
    "ad2.power(True)\n",
    "i2c = tdwf.I2Cbus(ad2.hdwf)\n",
    "devs = i2c.scan()\n",
    "for dev in devs:\n",
    "    print(f\"Found device @SAD: 0x{dev:02x}\")\n",
    "ad2.close()"
   ]
  }
